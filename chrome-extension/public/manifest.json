{
    "manifest_version":3,
    "name": "meeting Enhancer",
    "description": "Ai assisted Meeting Enhancer",
    "version":"0.0.1",
    "action": {
        "default_popup":"index.html",
        "default_title": "open"
    },
    "permissions":[
        "scripting"
    ],
    "host_permissions":[
        "https://meet.google.com/*"
    ]
}